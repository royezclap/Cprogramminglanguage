#include <stdio.h>
#include <math.h>
int main(){
  int a[]={3,-10,9,-2,-5,-1,2,6,3,-3};
  int counter=1,b,c,e=0;
  printf("b=");
  for(;counter<=10;counter++,e++){
    if(a[e]>0)
      printf("%i", a[e]);
  }
  printf("\nc=");
  for (e=0,counter=1;counter<=10;counter++,e++){
    if(a[e]<0)
     printf("%i", a[e]);
  }
}
